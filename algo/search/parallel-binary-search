orm="translate(8723.3,0)"><use data-c="1D45C" xlink:href="#MJX-27-TEX-I-1D45C"></use></g><g data-mml-node="mi" transform="translate(9208.3,0)"><use data-c="1D454" xlink:href="#MJX-27-TEX-I-1D454"></use></g><g data-mml-node="mo" transform="translate(9685.3,0)"><use data-c="28" xlink:href="#MJX-27-TEX-N-28"></use></g><g data-mml-node="mi" transform="translate(10074.3,0)"><use data-c="1D43E" xlink:href="#MJX-27-TEX-I-1D43E"></use></g><g data-mml-node="mo" transform="translate(10963.3,0)"><use data-c="29" xlink:href="#MJX-27-TEX-N-29"></use></g><g data-mml-node="mo" transform="translate(11574.6,0)"><use data-c="D7" xlink:href="#MJX-27-TEX-N-D7"></use></g><g data-mml-node="mi" transform="translate(12574.8,0)"><use data-c="1D459" xlink:href="#MJX-27-TEX-I-1D459"></use></g><g data-mml-node="mi" transform="translate(12872.8,0)"><use data-c="1D45C" xlink:href="#MJX-27-TEX-I-1D45C"></use></g><g data-mml-node="mi" transform="translate(13357.8,0)"><use data-c="1D454" xlink:href="#MJX-27-TEX-I-1D454"></use></g><g data-mml-node="mo" transform="translate(13834.8,0)"><use data-c="28" xlink:href="#MJX-27-TEX-N-28"></use></g><g data-mml-node="mi" transform="translate(14223.8,0)"><use data-c="1D440" xlink:href="#MJX-27-TEX-I-1D440"></use></g><g data-mml-node="mo" transform="translate(15274.8,0)"><use data-c="29" xlink:href="#MJX-27-TEX-N-29"></use></g><g data-mml-node="mo" transform="translate(15663.8,0)"><use data-c="29" xlink:href="#MJX-27-TEX-N-29"></use></g></g></g></svg>, tất nhiên là quá giới hạn thời gian.</p>
<pre class="prismjs line-numbers" data-start="1" v-pre="true"><code class="language-cpp=">#define For(i, a, b) for (int i = a; i &lt;= b; ++i)

int n, m, k;
int owner[N], req[N], L[N], R[N], A[N], l[N], r[N];
vector&lt;int&gt; pos[N];
BIT bit;

void update_info(int i, int d) {
    if (L[i] &lt;= R[i]) {
        bit.add(L[i], R[i], d * A[i]);
    }
    else {
        bit.add(L[i], m, d * A[i]);
        bit.add(1, R[i], d * A[i]);
    }
}

bool check(int o, int k) {
    For(i, 1, k) {
        update_info(i, 1);
    }
    long long cnt = 0;
    for (int i : pos[o]) {
        cnt += bit.get(i);
        cnt = min(cnt, (long long)req[o] + 1);
    }
    For(i, 1, k) {
        update_info(i, -1);
    }
    return cnt &gt;= req[o];
}

int main() {
    cin &gt;&gt; n &gt;&gt; m;
    For(i, 1, m) cin &gt;&gt; owner[i];
    For(i, 1, n) cin &gt;&gt; req[i];

    cin &gt;&gt; k;
    For(i, 1, k) cin &gt;&gt; L[i] &gt;&gt; R[i] &gt;&gt; A[i];

    For(i, 1, n) {
        l[i] = 0, r[i] = k + 1;
    }
    For(i, 1, m) {
        pos[owner[i]].push_back(i);
    }

    bit.makeBIT(m);
    For(i, 1, n) {
        int l = 0, r = k + 1;
        while (l &lt; r) {
            int g = (l + r) / 2;
            if (check(i, g)) r = g;
            else l = g + 1;
        }
        if (r &gt; k) cout &lt;&lt; "NIE\n";
        else cout &lt;&lt; r &lt;&lt; "\n";
    }

    return 0;
}
</code></pre>
<h4 id="tối-ưu" class="toc-header"><a class="toc-anchor" href="#tối-ưu">¶</a> Tối ưu</h4>
<p>Gọi <svg style="vertical-align: -0.566ex;" xmlns="http://www.w3.org/2000/svg" width="10.257ex" height="2.262ex" role="img" focusable="false" viewBox="0 -750 4533.7 1000" xmlns:xlink="http://www.w3.org/1999/xlink"><defs><path id="MJX-28-TEX-I-1D450" d="M34 159Q34 268 120 355T306 442Q362 442 394 418T427 355Q427 326 408 306T360 285Q341 285 330 295T319 325T330 359T352 380T366 386H367Q367 388 361 392T340 400T306 404Q276 404 249 390Q228 381 206 359Q162 315 142 235T121 119Q121 73 147 50Q169 26 205 26H209Q321 26 394 111Q403 121 406 121Q410 121 419 112T429 98T420 83T391 55T346 25T282 0T202 -11Q127 -11 81 37T34 159Z"></path><path id="MJX-28-TEX-I-210E" d="M137 683Q138 683 209 688T282 694Q294 694 294 685Q294 674 258 534Q220 386 220 383Q220 381 227 388Q288 442 357 442Q411 442 444 415T478 336Q478 285 440 178T402 50Q403 36 407 31T422 26Q450 26 474 56T513 138Q516 149 519 151T535 153Q555 153 555 145Q555 144 551 130Q535 71 500 33Q466 -10 419 -10H414Q367 -10 346 17T325 74Q325 90 361 192T398 345Q398 404 354 404H349Q266 404 205 306L198 293L164 158Q132 28 127 16Q114 -11 83 -11Q69 -11 59 -2T48 16Q48 30 121 320L195 616Q195 629 188 632T149 637H128Q122 643 122 645T124 664Q129 683 137 683Z"></path><path id="MJX-28-TEX-I-1D452" d="M39 168Q39 225 58 272T107 350T174 402T244 433T307 442H310Q355 442 388 420T421 355Q421 265 310 237Q261 224 176 223Q139 223 138 221Q138 219 132 186T125 128Q125 81 146 54T209 26T302 45T394 111Q403 121 406 121Q410 121 419 112T429 98T420 82T390 55T344 24T281 -1T205 -11Q126 -11 83 42T39 168ZM373 353Q367 405 305 405Q272 405 244 391T199 357T170 316T154 280T149 261Q149 260 169 260Q282 260 327 284T373 353Z"></path><path id="MJX-28-TEX-I-1D458" d="M121 647Q121 657 125 670T137 683Q138 683 209 688T282 694Q294 694 294 686Q294 679 244 477Q194 279 194 272Q213 282 223 291Q247 309 292 354T362 415Q402 442 438 442Q468 442 485 423T503 369Q503 344 496 327T477 302T456 291T438 288Q418 288 406 299T394 328Q394 353 410 369T442 390L458 393Q446 405 434 405H430Q398 402 367 380T294 316T228 255