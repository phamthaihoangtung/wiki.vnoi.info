<pre class="prismjs line-numbers" data-start="1" v-pre="true"><code class="language-cpp">void addString(const string &amp;s) {
   Node *p = root;
   REP(i, s.size()) {
      p = p-&gt;child[s[i] - 'a'];
      if (p == NULL) p = new Node();
      p-&gt;high = i + 1;
   }
}

